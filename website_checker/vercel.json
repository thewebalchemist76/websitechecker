{
    "builds": [{
        "src": "website_checker/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        { "rewrites": [{ "source": "/(.*)", "destination": "/" }] }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "website_checker/wsgi.py"
        }
    ]
}
